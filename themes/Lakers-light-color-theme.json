    "badge.background": "#552583",
    "badge.foreground": "#fffde4",

    "dropdown.background": "#ffffff",
    "dropdown.foreground": "#24135f",
    "dropdown.border": "#fdb927",

    "button.background": "#552583",
    "button.foreground": "#fffde4",
    "button.hoverBackground": "#6a3a94",

    "input.background": "#ffffff",
    "input.foreground": "#24135f",
    "input.border": "#fdb927",
