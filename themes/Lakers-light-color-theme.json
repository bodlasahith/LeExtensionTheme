    "scrollbarSlider.background": "#fdb92744",
    "scrollbarSlider.hoverBackground": "#fdb92766",
    "scrollbarSlider.activeBackground": "#fdb92788",
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {"fontStyle": "italic", "foreground": "#bdbdbd"}
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {"foreground": "#817800"}
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {"foreground": "#ffb922"}
    },
    {
      "name": "Function, Special Method",
      "scope": ["entity.name.function", "meta.function-call", "variable.function", "support.function", "keyword.other.special-method"],
      "settings": {"foreground": "#552583"}
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {"foreground": "#99b66e"}
    },
    {
      "name": "Number, Constant",
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": {"foreground": "#F78C6C"}
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag"],
      "settings": {"foreground": "#f07178"}
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {"foreground": "#C792EA"}
    },
    {
      "name": "Class, Support",
      "scope": ["support.class", "support.type"],
      "settings": {"foreground": "#a96e00"}
    }
  ]
}
